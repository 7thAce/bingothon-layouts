{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type":"object",
    "additionalProperties": {
        "type":"object",
        "required": ["baseVolume","fading"],
        "additionalProperties": false,
        "properties": {
            "baseVolume": {"type":"number"},
            "fading": {
                "type": "string",
                "enum": ["fadein","fadeout","muted","unmuted"]
            }
        }
    }
}