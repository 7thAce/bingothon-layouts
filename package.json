{
  "name": "bingothon-layouts",
  "version": "1.0.0",
  "description": "Layouts for Bingothon using the template for NodeCG bundles that use Vue.js and TypeScript.",
  "homepage": "https://github.com/bingothon/bingothon-layouts",
  "bugs": {
    "url": "https://github.com/bingothon/bingothon-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingothon/bingothon-layouts.git"
  },
  "license": "MIT",
  "author": "Bingothon",
  "contributors": [
    "lepelog",
    "floha258"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts src/browser_shared",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "start": "node ../..",
    "typeschemas": "node script/typeschemas.js",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w",
    "tscver": "tsc --version"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.13",
    "@types/d3": "^5.7.2",
    "@types/deep-equal": "^1.0.1",
    "@types/request-promise": "^4.1.44",
    "@types/tmi.js": "^1.4.0",
    "@types/webpack-env": "^1.15.3",
    "@types/ws": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/component-compiler-utils": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "globby": "^11.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.5.0",
    "json-schema-to-typescript": "^7.0.0",
    "mini-css-extract-plugin": "^1.2.1",
    "nodecg": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "trash-cli": "^3.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.5.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.9.5",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.1.0"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "nodecg": {
    "compatibleRange": "^1.5.0",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.0.0"
    },
    "dashboardPanels": [
      {
        "name": "bingo-control",
        "title": "Bingo Control",
        "file": "bingo-control.html",
        "width": 2,
        "workspace": "Bingothon"
      },
      {
        "name": "interview-control",
        "title": "Interview Control",
        "file": "interview-control.html",
        "width": 2,
        "workspace": "Bingothon"
      },
      {
        "name": "camnames-control",
        "title": "Cam Names Control",
        "file": "camnames-control.html",
        "width": 2,
        "workspace": "Bingothon"
      },
      {
        "name": "layout-control",
        "title": "Layout Control",
        "file": "layout-control.html",
        "width": 2,
        "workspace": "Bingothon"
      },
      {
        "name": "twitch-control",
        "title": "Twitch Control",
        "file": "twitch-control.html",
        "width": 2,
        "workspace": "Bingothon"
      },
      {
        "name": "obs-control",
        "title": "OBS Control",
        "file": "obs-control.html",
        "width": 2
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "file": "checklist.html",
        "width": 2
      },
      {
        "name": "twitch-resizer-dashboard",
        "title": "Twitch Resizer",
        "width": 10,
        "file": "crop-control.html",
        "headerColor": "#008BB9",
        "workspace": "Twitch Resizing"
      }
    ],
    "graphics": [
      {
        "file": "game-layout.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "intermission.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "host-dashboard.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "exploration-bingo.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "interview.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "camnames.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "omnibar.html",
        "width": 1920,
        "height": 80
      }
    ],
    "mount": [
      {
        "directory": "static",
        "endpoint": "static"
      }
    ]
  },
  "vue": {
    "runtimeCompiler": true
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@mdi/font": "^5.8.55",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^5.0.0",
    "d3": "^3.5.17",
    "deep-equal": "^1.0.1",
    "discord.js": "^12.3.1",
    "fibers": "^5.0.0",
    "gsap": "^2.1.3",
    "mpc-js": "^1.2.1",
    "node-opus": "^0.3.3",
    "obs-websocket-js": "^3.1.0",
    "request-promise": "^4.2.4",
    "sass": "^1.28.0",
    "tmi.js": "^1.5.0",
    "ts-loader": "^8.0.7",
    "vuetify": "^2.3.16",
    "vuetify-loader": "^1.6.0",
    "vuex": "^3.1.1",
    "webpack-livereload-plugin": "^2.3.0",
    "ws": "^7.1.2"
  }
}
